<%- include("partials/header") -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="composeTitle">Title</label>
            <input type="text" name="composeTitle" autocomplete="off" class="form-control">
            <label for="composePost">Post</label>
            <textarea name="composePost" rows="4" cols="50" class="form-control"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Publish</button>
    </form>
<%- include("partials/footer") -%>